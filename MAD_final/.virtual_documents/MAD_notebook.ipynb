def cell_into_markers(cells):
    markers = {}
    for cell in cells:
        res = ""
        args = cell.split()
        for arg in args:
            if arg in cell_type_dict:
                res += " ".join(cell_type_dict[arg]) + " "
            else:
                res += arg
                if arg != args[-1]:
                    res += " "
        markers[cell] = res
    return markers



